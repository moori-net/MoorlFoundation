<div class="sw-cms-el-config-moorl-contact">
    <sw-cms-inherit-wrapper
            field="email"
            :element="element"
            :label="$tc('moorl-foundation.field.email')"
    >
        <template #default="{ isInherited }">
            <sw-cms-mapping-field
                    v-model:config="element.config.email"
                    value-types="string"
            >
                <mt-text-field
                        :label="$tc('moorl-foundation.field.email')"
                        v-model="element.config.email.value"
                        :disabled="isInherited"
                />
            </sw-cms-mapping-field>
        </template>
    </sw-cms-inherit-wrapper>

    <sw-cms-inherit-wrapper
            field="phoneNumber"
            :element="element"
            :label="$tc('moorl-foundation.field.phoneNumber')"
    >
        <template #default="{ isInherited }">
            <sw-cms-mapping-field
                    v-model:config="element.config.phoneNumber"
