<div class="sw-cms-el-config-moorl-address">
    <sw-cms-inherit-wrapper
            field="street"
            :element="element"
            :label="$tc('moorl-foundation.field.street')"
    >
        <template #default="{ isInherited }">
            <sw-cms-mapping-field
                    v-model:config="element.config.street"
                    value-types="string"
            >
                <mt-text-field
                        :label="$tc('moorl-foundation.field.street')"
                        v-model="element.config.street.value"
                        :disabled="isInherited"
