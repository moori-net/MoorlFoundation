{% block moorl_entity_grid_card %}
    <sw-card class="moorl-entity-grid">
        <template v-for="(_, name) in $slots" v-slot:[name]="slotData">
            <slot :name="name" v-bind="slotData" />
        </template>

        <moorl-entity-grid
                :key="$attrs.entity"
                v-bind="$attrs"
        >
            <template v-for="(_, name) in $slots" v-slot:[name]="slotData">
                <slot :name="name" v-bind="slotData" />
            </template>
        </moorl-entity-grid>
    </sw-card>
{% endblock %}
