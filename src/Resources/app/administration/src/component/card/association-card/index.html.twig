{% block moorl_association_card %}
    <sw-card class="moorl-association-card" :title="$tc('moorl-foundation.card.association')">
        {% block moorl_association_card_inner %}
            <sw-text-field :label="$tc('moorl-foundation.field.street')" v-model:value="item.street"/>
            <sw-text-field :label="$tc('moorl-foundation.field.streetNumber')" v-model:value="item.streetNumber"/>
            <sw-text-field :label="$tc('moorl-foundation.field.zipcode')" v-model:value="item.zipcode"/>
            <sw-text-field :label="$tc('moorl-foundation.field.city')" v-model:value="item.city"/>
            <sw-text-field :label="$tc('moorl-foundation.field.additionalAddressLine1')" v-model:value="item.additionalAddressLine1"/>
            <sw-text-field :label="$tc('moorl-foundation.field.additionalAddressLine2')" v-model:value="item.additionalAddressLine2"/>
            <sw-entity-single-select :label="$tc('moorl-foundation.field.country')" v-model:value="item.countryId"  entity="country"/>
            <sw-entity-single-select :label="$tc('moorl-foundation.field.countryState')" v-model:value="item.countryStateId"  entity="country_state"/>
            <sw-text-field :label="$tc('moorl-foundation.field.countryCode')" v-model:value="item.countryCode"/>
            <sw-text-field :label="$tc('moorl-foundation.field.locationPlaceId')" v-model:value="item.locationPlaceId"/>
        {% endblock %}

        <slot name="append"/>
    </sw-card>
{% endblock %}
