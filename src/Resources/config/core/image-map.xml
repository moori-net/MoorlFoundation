<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="MoorlFoundation\Core\Content\ImageMap\ImageMapDefinition">
            <tag name="shopware.entity.definition" entity="moorl_image_map"/>
        </service>

        <service id="MoorlFoundation\Core\Content\ImageMap\ImageMapTranslationDefinition">
            <tag name="shopware.entity.definition" entity="moorl_image_map_translation"/>
        </service>

        <service id="MoorlFoundation\Core\Content\ImageMap\ImageMapItemDefinition">
            <tag name="shopware.entity.definition" entity="moorl_image_map_item"/>
        </service>

        <service id="MoorlFoundation\Core\Content\ImageMap\ImageMapItemTranslationDefinition">
            <tag name="shopware.entity.definition" entity="moorl_image_map_item_translation"/>
        </service>
    </services>
</container>
