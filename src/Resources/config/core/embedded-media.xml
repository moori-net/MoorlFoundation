<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="MoorlFoundation\Core\Content\EmbeddedMedia\EmbeddedMediaDefinition">
            <tag name="shopware.entity.definition" entity="moorl_media"/>
        </service>

        <service id="MoorlFoundation\Core\Content\EmbeddedMedia\EmbeddedMediaTranslationDefinition">
            <tag name="shopware.entity.definition" entity="moorl_media_translation"/>
        </service>

        <service id="MoorlFoundation\Core\Content\EmbeddedMedia\EmbeddedMediaLanguageDefinition">
            <tag name="shopware.entity.definition" entity="moorl_media_language"/>
        </service>

        <service id="MoorlFoundation\Core\Content\Language\LanguageExtension">
            <tag name="shopware.entity.extension"/>
        </service>
    </services>
</container>
